syslog=yes				            # log update msgs to syslog
pid=/var/run/ddclient/ddclient.pid	# record PID in file.
ssl=yes 				            # use ssl-support.  Works with
use=web, web=api.ipify.org

# authelia
protocol=cloudns, \
dynurl={{ authelia_ddns }}, \
auth.{{ domain }}

# sftpgo
protocol=cloudns, \
dynurl={{ sftpgo_ddns }}
sftpgo.{{ domain }}
